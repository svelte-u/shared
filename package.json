{
    "name": "@sveu/shared",
    "version": "0.10.1",
    "packageManager": "pnpm@8.4.0",
    "description": "Shared utilities for svelte utility libraries",
    "author": "Mohamed Nesredin<https://github.com/Mohamed-Kaizen>",
    "license": "MIT",
    "homepage": "https://github.com/svelte-u/shared#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/svelte-u/shared.git"
    },
    "bugs": {
        "url": "https://github.com/svelte-u/shared/issues"
    },
    "keywords": [
        "svelte",
        "utils"
    ],
    "type": "module",
    "scripts": {
        "build": "pnpm run clean && tsx scripts/build.ts && tsup",
        "clean": "tsx scripts/clean.ts",
        "git:add": "pnpm run build && pnpm run clean && git add .",
        "commit": "cz commit",
        "lint": "eslint .",
        "lint:fix": "pnpm run lint --fix",
        "release": "bumpp",
        "release:next": "bumpp --tag next",
        "test": "vitest run",
        "test:watch": "vitest --watch"
    },
    "exports": {
        ".": {
            "import": "./index.js"
        },
        "./curry": {
            "import": "./curry.js"
        },
        "./dicts": {
            "import": "./dicts.js"
        },
        "./lists": {
            "import": "./lists.js"
        },
        "./math": {
            "import": "./math.js"
        }
    },
    "devDependencies": {
        "@trivago/prettier-plugin-sort-imports": "^4.1.1",
        "@types/fs-extra": "^11.0.1",
        "@types/node": "^20.3.3",
        "@typescript-eslint/eslint-plugin": "^5.60.1",
        "@typescript-eslint/parser": "^5.60.1",
        "bumpp": "^9.1.1",
        "eslint": "^8.44.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-tsdoc": "^0.2.17",
        "fast-glob": "^3.3.0",
        "fs-extra": "^11.1.1",
        "prettier": "^2.8.8",
        "svelte": "^4.0.1",
        "tsup": "^7.1.0",
        "tsx": "^3.12.7",
        "typescript": "^5.1.6",
        "vitest": "^0.32.2"
    },
    "publishConfig": {
        "access": "public"
    }
}
