{
    "name": "@sveu/shared",
    "version": "0.5.4",
    "packageManager": "pnpm@7.20.0",
    "description": "Shared utilities for svelte utility libraries",
    "author": "Mohamed Nesredin<https://github.com/Mohamed-Kaizen>",
    "license": "MIT",
    "homepage": "https://github.com/svelte-u/shared#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/svelte-u/shared.git"
    },
    "bugs": {
        "url": "https://github.com/svelte-u/shared/issues"
    },
    "keywords": [
        "svelte",
        "utils"
    ],
    "type": "module",
    "scripts": {
        "build": "pnpm run clean && tsx scripts/build.ts && tsup",
        "clean": "tsx scripts/clean.ts",
        "git:add": "pnpm run build && pnpm run clean && git add .",
        "commit": "cz commit",
        "lint": "eslint .",
        "lint:fix": "pnpm run lint --fix",
        "release": "bumpp --all",
        "test": "vitest run",
        "test:watch": "vitest --watch"
    },
    "exports": {
        ".": {
            "import": "./index.js"
        },
        "./curry": {
            "import": "./curry.js"
        },
        "./dicts": {
            "import": "./dicts.js"
        },
        "./math": {
            "import": "./math.js"
        }
    },
    "devDependencies": {
        "@trivago/prettier-plugin-sort-imports": "^4.0.0",
        "@types/fs-extra": "^11.0.1",
        "@types/node": "^18.13.0",
        "@typescript-eslint/eslint-plugin": "^5.51.0",
        "@typescript-eslint/parser": "^5.51.0",
        "bumpp": "^8.2.1",
        "eslint": "^8.33.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-tsdoc": "^0.2.17",
        "fast-glob": "^3.2.12",
        "fs-extra": "^11.1.0",
        "prettier": "^2.8.4",
        "svelte": "^3.55.1",
        "tsup": "^6.6.0",
        "tsx": "^3.12.3",
        "typescript": "^4.9.5",
        "vitest": "^0.28.4"
    },
    "publishConfig": {
        "access": "public"
    }
}
